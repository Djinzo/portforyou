{
   "usesTypeScript": true,
   "usesCssModule": true,
   "cssPreprocessor": "styled",
   "testLibrary": "Testing Library",
   "component": {
      "default": {
         "customTemplates": {
            "component": ".templates/component/TemplateName.tsx",
            "style": ".templates/component/TemplateName.style.tsx",
            "test": ".templates/component/TemplateName.test.tsx"
         },
         "path": "components",
         "withStyle": true,
         "withTest": true,
         "withStory": false,
         "withLazy": false
      },
      "page": {
         "customTemplates": {
            "component": ".templates/page/TemplateName.tsx",
            "style": ".templates/page/TemplateName.style.tsx",
            "test": ".templates/page/TemplateName.test.tsx"
         },
         "path": "pages",
         "withStyle": true,
         "withTest": true,
         "withStory": false,
         "withLazy": false
      }
   }
}
